    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.2.5",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "dot-prop": "^6.0.1",
    "dotenv": "^9.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
